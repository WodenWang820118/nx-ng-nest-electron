{
  "name": "elec-ng-nest",
  "version": "0.0.0",
  "main": "main.js",
  "license": "MIT",
  "type": "module",
  "author": "Guan Xin Wang",
  "description": "An Electron application built Angular and Nest",
  "scripts": {
    "dev-front": "nx serve ng-tracker",
    "dev-back": "cross-env-shell NODE_ENV=dev PORT=3000 \"nx serve nest-backend --watch\"",
    "dev-electron": "cross-env-shell NODE_ENV=dev \"npm run build && electron-forge start\"",
    "build-front": "nx build ng-tracker --configuration=development --optimization --base-href ./",
    "build-back": "cross-env-shell NODE_ENV=dev PORT=3000 \"nx build nest-backend\"",
    "build": "npm run build-front && npm run build-back",
    "build-prod-front": "nx build ng-tracker --configuration production --optimization --base-href ./",
    "build-prod-back": "cross-env-shell NODE_ENV=prod PORT=5000 \"nx build nest-backend\"",
    "build-prod": "npm run build-prod-front && npm run build-prod-back && node hooks.js",
    "make": "cross-env-shell NODE_ENV=prod PORT=5000 \"npm run build-prod && electron-forge make\"",
    "package": "cross-env-shell NODE_ENV=prod \"npm run build-prod && electron-forge package\""
  },
  "private": true,
